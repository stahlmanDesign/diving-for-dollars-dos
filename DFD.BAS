SUB TOUCH
 REM "DOES FISH HURT MAN?"
     IF POINT(AA + 5, BB) = 2 THEN B = 2: AI = AI - 1: SOUND 49, 1
     IF POINT(AA + 9, BB) = 2 THEN B = 1: AI = AI - 1: SOUND 49, 1
     IF POINT(AA + 9, BB + 5) = 2 THEN B = 4: AI = AI - 1: SOUND 49, 1
     IF POINT(AA + 9, BB + 9) = 2 THEN B = 7: AI = AI - 1: SOUND 49, 1
     IF POINT(AA + 5, BB + 9) = 2 THEN B = 8: AI = AI - 1: SOUND 49, 1
     IF POINT(AA, BB + 9) = 2 THEN B = 9: AI = AI - 1: SOUND 49, 1
     IF POINT(AA, BB + 5) = 2 THEN B = 6: AI = AI - 1: SOUND 49, 1
     IF POINT(AA, BB) = 2 THEN B = 3: AI = AI - 1: SOUND 49, 1
     END SUB

