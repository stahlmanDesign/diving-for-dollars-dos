SUB FISHLEFT
REM FISH GOES LEFT ACROSS SCREEN
     IF FI = 0 THEN PUT (F1, F2), N: FI = 1
     PUT (F1, F2), N
     IF BB > F2 THEN F2 = F2 + (INT(RND(1) * 2 + 1))
     IF BB < F2 THEN F2 = F2 - (INT(RND(1) * 2 + 1))
     F1 = F1 - MS * 2.5
     PUT (F1, F2), N
     IF F1 < 15 THEN PUT (F1, F2), N: FI = 0: RANDOMIZE TIMER: F2 = INT(RND(1) * FD): F1 = 300: EXIT SUB
     IF F2 > 180 THEN PUT (F1, F2), N: FI = 0: RANDOMIZE TIMER: F2 = INT(RND(1) * FD): F1 = 300: EXIT SUB
     IF F2 < 15 THEN PUT (F1, F2), N: FI = 0: RANDOMIZE TIMER: F2 = INT(RND(1) * FD): F1 = 300: EXIT SUB
     TOUCH

END SUB

